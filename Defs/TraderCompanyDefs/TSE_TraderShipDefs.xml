<Defs>
	<TraderShipsExpanded.TraderCompanyDef>
		<defName>TSE_Company_OrbitalStation</defName>
		<label>Orbital Station</label>
		<iconPath>TSE/Factions/Icon</iconPath>
		<iconColor>(255,255,100)</iconColor>
		<shipThingDefs>
			<li>TSE_TraderShip_Cutlass</li>
			<li>TSE_TraderShip_Cargo</li>
			<li>TSE_TraderShip_Swordfish</li>
			<li>TSE_TraderShip_Mule</li>
			<li>TSE_TraderShip_Mace</li>
		</shipThingDefs>
		<requestCostForAll>400</requestCostForAll> <!-- Override prices. Empty traderKinds = all traders are available -->
		<!--
		<traderKinds>
			<li>
				<traderKindDef>...</traderKindDef>
				<requestCost>...</requestCost>
			</li>
		</traderKinds>
		-->
		<excludedTraderKinds>
			<li>TSE_ProductionDepartment</li>
			<li>TSE_MiningDepartment</li>
			<li>TSE_MedicalDepartment</li>
			<li>TSE_MilitaryDepartment</li>
		</excludedTraderKinds>
	</TraderShipsExpanded.TraderCompanyDef>
	
	<TraderShipsExpanded.TraderCompanyDef>
		<defName>TSE_Company_GalaxyTrader</defName>
		<label>Galaxy Trading Corp Station</label>
		<iconPath>TSE/Factions/Icon</iconPath>
		<iconColor>(255,100,255)</iconColor>
		<factionDef>TSE_Faction_GTC</factionDef>
		<shipThingDefs>
			<li>TSE_TraderShip_Tortoise</li> <!-- ADD THE SHIP THINGIE -->
			<li>TSE_TraderShip_TortoiseNew</li>
			<li>TSE_TraderShip_Cargo</li>
			<!--
			<li>TSE_TraderShip_Swordfish</li>
			<li>TSE_TraderShip_Mule</li>
			<li>TSE_TraderShip_Mace</li>
			-->
		</shipThingDefs>
		<traderKinds>
			<li>
				<traderKindDef>TSE_ProductionDepartment</traderKindDef>
				<requestCost>300</requestCost>
			</li>
			<li>
				<traderKindDef>TSE_MiningDepartment</traderKindDef>
				<requestCost>300</requestCost>
			</li>
			<li>
				<traderKindDef>TSE_MedicalDepartment</traderKindDef>
				<requestCost>400</requestCost>
			</li>
			<li>
				<traderKindDef>TSE_MilitaryDepartment</traderKindDef>
				<requestCost>500</requestCost>
			</li>
		</traderKinds>
	</TraderShipsExpanded.TraderCompanyDef>
	
	
